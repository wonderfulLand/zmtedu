<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="zm.zmtedu.dao.UserDao">
    <insert id="saveUser" parameterType="zm.zmtedu.entity.User">
        INSERT INTO user_sys(username,gender,pwd)
        VALUES(#{userName},#{gender},#{pwd})
    </insert>
</mapper>










